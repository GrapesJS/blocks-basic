{
  "name": "grapesjs-blocks-basic",
  "version": "0.1.1",
  "description": "Basic blocks for the GrapesJS editor",
  "main": "dist/grapesjs-blocks-basic.min.js",
  "scripts": {
    "lint": "eslint src",
    "build": "WEBPACK_ENV=prod && npm run v:patch && webpack",
    "v:patch": "npm version --no-git-tag-version patch",
    "start": "WEBPACK_ENV=dev ./node_modules/.bin/webpack-dev-server --progress --colors"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "blocks",
    "basic"
  ],
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "grapesjs": "^0.8.4",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
